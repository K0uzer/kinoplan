{
    "compilerOptions": {
        "types": [],
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "plugins": [
            {
                "name": "next"
            }
        ],
        "baseUrl": ".",
        "paths": {
            "@/*": ["./*"],
            "@upOneDr/*": ["./../*"],
            "@upTwoDr/*": ["./../../*"],
            "@upThreeDr/*": ["./../../../*"],
            "@/app/*": ["app/*"],
            "@/components/*": ["components/*"],
            "@/api/*": ["API/*"]
        }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
    "exclude": ["node_modules"]
}